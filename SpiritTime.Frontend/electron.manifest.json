{
  "executable": "SpiritTime.Frontend",
  "splashscreen": {
    "imageFile": "bin/Assets/icon32.png"
  },
  "name": "SpiritTime",
  "author": "Sindia Höller",
  "singleInstance": false,
  "build": {
    "appId": "com.SpiritTime.Frontend.app",
    "productName": "SpiritTime",
    "copyright": "Copyright ©SpiritTime 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": ["**/*"]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": ["**/*"]
      },
      "**/*"
    ],
    "win": {
      "icon": "bin/Assets/icon.ico"
    },
    "osx": {
      "icon": "bin/Assets/icon.icns"
    },
    "linux": {
      "icon": "bin/Assets/icon_120x120.png"
    }
  }
}