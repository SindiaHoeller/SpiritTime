{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery@3.5.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.slim.min.js"
      ]
    },
    {
      "library": "popper.js@1.16.1",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "popper.min.js",
        "umd/popper-utils.js",
        "umd/popper-utils.js.map",
        "umd/popper-utils.min.js",
        "umd/popper-utils.min.js.map",
        "umd/popper.js",
        "umd/popper.js.map",
        "umd/popper.min.js",
        "umd/popper.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.min.js.map"
      ]
    }
  ]
}